<UserControl x:Class="CollectorsFrontEnd.CollectionViewGeneralization"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CollectorsFrontEnd"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ListView x:Name="LVItems" Grid.ZIndex="1">
            <ListView.View>
                <GridView x:Name="GVColumns"/>
            </ListView.View>
            <ListView.ItemTemplate>
                <DataTemplate>
                    <local:CObjectListDisplay></local:CObjectListDisplay>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </Grid>
</UserControl>
