<UserControl x:Class="CollectorsFrontEnd.Interfaces.Subs.CompSubKeyValCreater"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CollectorsFrontEnd.Interfaces.Subs"
             mc:Ignorable="d" 
              Height="72" Width="190">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <TextBox Grid.Column="0" Text="{Binding Path=Key, Mode=TwoWay,
                        UpdateSourceTrigger=PropertyChanged}"></TextBox>
        <TextBox Grid.Column="1" Text="{Binding Path=Value, Mode=TwoWay,
                        UpdateSourceTrigger=PropertyChanged}"></TextBox>
        <Button Grid.Column="0" Grid.Row="1" Content="OK" Click="eOK_Click"></Button>
        <Button Grid.Column="1" Grid.Row="1" Content="Cancel" Click="eCancel_Click"></Button>
    </Grid>
</UserControl>
